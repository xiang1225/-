/* pages/ssignin/ssign.wxss */
page {
  height: 100%;
}

.tsignin {
  height: 100%;
}

.tsignin-inner {
  height: 100%;
}

.table {
  border: 0px solid darkgray;
  font-size: 12px;
  height: 100%;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 2rem;
  align-items: center;
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
}

.bg-w {
  background: rgb(38, 202, 137);
  height: 10%;
}

.th {
  width: 40%;
  justify-content: center;
  background: rgb(38, 202, 137);
  color: #fff;
  display: flex;
  font-size: 14px;
  align-items: center;
}
.content{
  height: 90%;
}